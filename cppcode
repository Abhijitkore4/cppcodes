pattern:
****
 ***
  **
   *
  

int n;
cin>>n;

int i=1;
while(i<=n)
{
    //spaces print karlo
    int space=i-1;
    while(space)
    {
        cout<<" ";
        space=space-1;
    }

    // stars print karlo
    int k=1;
    while(k<=n-i+1)
    {
        cout<<"*";
        k=k+1;
    }
    i=i+1;
    cout<<endl;
}

pattern: 
     1
    121
   12321


int n;
cin>>n;

int i=1;
while(i<=n)
{
    //print spaces (1st triangle)
    int j=1;
    while(j<=n-i)
    {
        cout<<" ";
        j=j+1;
    }


    //print 2nd triangle
    int k=1;
    int p=1;
    while(k<=i)
    {
        cout<<p;
        p=p+1;
        k=k+1;
    }


    //print 3rd triangle
    int l=1;
    int s=i-1;
    while(l<=i-1)
    {
        cout<<s;
        s=s-1;
        l=l+1;
    }

i=i+1;
cout<<endl;


}



dabang pattern:
1234554321
1234**4321
123****321
12******21
1********1


*/

int n;
cin>>n;

int i=1;
while(i<=n)
{
    //print first triangle
    int j=1;

    while(j<=n-i+1)
    {
        cout<<j;
        j=j+1;
    }
    // print second triangle (star)
    int k=1;
    while(k<=i-1)
    {
        cout<<"*";
        k=k+1;
    }
    // print third triangle
    int l=1;
    while(l<=i-1)
    {
        cout<<"*";
        l=l+1;
    }

    //print fourth triangle
    int ch=n-i+1;
    int m=1;
    while(m<=ch)
    {
        cout<<ch;
        ch=ch-1;
    }





i=i+1;
cout<<endl;
    
}


}
