int n;
cout<<"Enter no of coloumn: ";
cin>>n;

int i=1;
while(i<=n)
{
    //spaces print karlo
    int space=n-i; 
    while(space)
    {
        cout<<" ";
        space=space-1;

    }
    // stars print karlo
    int j=1;
    while(j<=i)
    {
        cout<<"*";
        j=j+1;
    }
    i=i+1;
    cout<<endl;
}


